    "description": "",
    "main": "static/js/index.js",
    "scripts": {
        "build": "webpack --config webpack_config/webpack.prod-config.cjs",
        "dev-build": "webpack --config webpack_config/webpack.dev-config.cjs"
    },
    "dependencies": {
        "open": "^9.1.0",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.4",
        "webpack": "^5.79.0",
        "webpack-cli": "^5.0.1",
        "webpack-merge": "^5.8.0"
    },
    "devDependencies": {
        "@parcel/config-default": "^2.8.3",
        "@types/dompurify": "^2.4.0",
