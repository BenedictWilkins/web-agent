[ req ]
prompt = no
encrypt_key = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext

[ dn ]
CN = localhost
O = DiceLab
OU = Web-Agent
L = Egham
ST = Surrey
C = UK

[ req_ext ]
subjectAltName = DNS: localhost, IP.1: 127.0.0.1, IP.2: ::1
keyUsage = digitalSignature
extendedKeyUsage = serverAuth
